{
    "LAZY_DATASET_READER": 0,
    "CUDA_DEVICE": 0,
    "EVALUATE_ON_TEST": 1,
    "NUM_EPOCHS": 50,
    "SEED": {
        "sampling strategy": "integer",
        "bounds": [0, 100000]
    },
    "THROTTLE": null,
    "USE_SPACY_TOKENIZER": 0,
    "FREEZE_EMBEDDINGS": null,
    "EMBEDDINGS": ["GLOVE"],
    "ENCODER": "CNN",
    "LEARNING_RATE": {
        "sampling strategy": "uniform",
        "bounds": [1e-6, 1e-1]
    },
    "DROPOUT": {
        "sampling strategy": "uniform",
        "bounds": [0, 0.5]
    },
    "BATCH_SIZE": 64,
    "MAX_FILTER_SIZE": {
        "sampling strategy": "integer",
        "bounds": [3, 6]
    },
    "HIDDEN_SIZE": {
        "sampling strategy": "integer",
        "bounds": [64, 512]
    },
    "NUM_FILTERS": {
        "sampling strategy": "integer", 
        "bounds": [64, 512]
    }
}
