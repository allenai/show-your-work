{
    "EVALUATE_ON_TEST": 1,
    "ACTIVATION": {
        "sampling strategy": "choice",
        "choices": ["relu", "tanh"]
    },
    "NUM_LAYERS": {
        "sampling strategy": "choice",
        "choices": [1, 2, 3]
    },
    "HIDDEN_DIM": {
        "sampling strategy": "integer",
        "bounds": [64, 512]
    },
    "DROPOUT": {
        "sampling strategy": "uniform",
        "bounds": [0, 0.5]
    },
    "BATCH_SIZE": 64,
    "NUM_EPOCHS": 140,
    "GRAD_NORM": {
        "sampling strategy": "uniform",
        "bounds": [5, 10]
    },
    "LEARNING_RATE": {
        "sampling strategy": "loguniform",
        "bounds": [1e-6, 1e-1]
    },
    "SEED": {
        "sampling strategy": "integer",
        "bounds": [0, 100000]
    }
}